      **************************************************************************
      **  CARGA DE DATOS PERSONALES Y CALCULO DE ASIGNACIONES Y DEDUCCIONES  ***
      **************************************************************************
      STORE PERSONAL->CEDULA    TO WCEDULA
      STORE PERSONAL->TURNO     TO WTURNO
      STORE PERSONAL->TIPO      TO WTIPO
      STORE PERSONAL->NOMBRE    TO WNOMBRE
      STORE PERSONAL->APELLIDO  TO WAPELLIDO
      STORE PERSONAL->CARGO     TO WCLASIF
      STORE PERSONAL->SUELDO    TO WBASICO
      STORE PERSONAL->BONOCO    TO WBONOCO
     
      STORE WBASICO/TABLA->F_HORASD TO WSALHRSD
      STORE WBASICO/TABLA->F_HORASM TO WSALHRSM
      STORE WBASICO/TABLA->F_HORASN TO WSALHRSN

      ****  DIAS ORDINARIOS TRABAJADOS  ******
      STORE PROCESO->P_DIASORD  TO WDIASTRABD
      STORE PROCESO->P_DIASORM  TO WDIASTRABM
      STORE PROCESO->P_DIASORN  TO WDIASTRABN
      STORE (WDIASTRABD+WDIASTRABM+WDIASTRABN) TO WDIASTRAB

      ****  HORAS ORDINARIAS TRABAJADAS *****
      STORE WDIASTRABD*TABLA->F_HORASD TO WHORASORD
      STORE WDIASTRABM*TABLA->F_HORASD TO WHORASORM
      STORE WDIASTRABN*TABLA->F_HORASD TO WHORASORN
      STORE WHORASORD+WHORASORM+WHORASORN TO WHORASOR

      ****  TIEMPO ORDINARIO DE TRABAJO EN BS ******************
      STORE (WBASICO/TABLA->F_HORASD)*WHORASORD TO WBSORDD
      STORE (WBASICO/TABLA->F_HORASD)*WHORASORM TO WBSORDM
      STORE (WBASICO/TABLA->F_HORASD)*WHORASORN TO WBSORDN
      STORE WBSORDD+WBSORDM+WBSORDN TO WBSORD

      **** REPOSO/COMIDA
      STORE  PROCESO->P_REPCOMD TO WREPCOMD
      STORE  PROCESO->P_REPCOMM TO WREPCOMM
      STORE  PROCESO->P_REPCOMN TO WREPCOMN
      STORE  WREPCOMD*((WBASICO+WBONOCO)/TABLA->F_HORASD)*TABLA->F_REPCOM TO WREPCOMDBS
      STORE  WREPCOMM*((WBASICO+WBONOCO)/TABLA->F_HORASM)*TABLA->F_REPCOM TO WREPCOMMBS
      STORE  WREPCOMN*((WBASICO+WBONOCO)/TABLA->F_HORASN)*TABLA->F_REPCOM TO WREPCOMNBS
      STORE  WREPCOMD+WREPCOMM+WREPCOMN       TO WREPCOM
      STORE  WREPCOMDBS+WREPCOMMBS+WREPCOMNBS TO WREPCOMBS

      **** BONO NOCTURNO DE GUARDIA MIXTA Y NOCTURNA
      STORE  PROCESO->P_BNGM TO WBNGM
      STORE  PROCESO->P_BNGN TO WBNGN
      STORE  WBNGM*((WBASICO+WBONOCO+TABLA->F_AYUCIU+(((WBASICO+WBONOCO)/TABLA->F_HORASM)/2)))/TABLA->F_HORASD*TABLA->F_BNOCTU TO WBNGMBS
      STORE  WBNGN*((WBASICO+WBONOCO+TABLA->F_AYUCIU+(((WBASICO+WBONOCO)/TABLA->F_HORASN)/2)))/TABLA->F_HORASD*TABLA->F_BNOCTU TO WBNGNBS
      STORE WBNGM+WBNGN     TO WBNGHR
      STORE WBNGMBS+WBNGNBS TO WBNGBS

      **** SOBRE TIEMPO DE GUARDIA MIXTA Y NOCTURNA
      STORE  PROCESO->P_STGM TO WSTGM
      STORE  PROCESO->P_STGN TO WSTGN
      IF WBNGM > 0
         STORE  ((WBNGMBS/WBNGM)*4) TO WUNDBNGM
      ELSE
         STORE 0 TO WUNDBNGM
      ENDIF
      IF WBNGN > 0
         STORE  ((WBNGNBS/WBNGN)*6) TO WUNDBNGN
      ELSE
         STORE 0 TO WUNDBNGN
      ENDIF
      STORE  WSTGM* ( (WBASICO+WBONOCO+TABLA->F_AYUCIU+ (((WBASICO+WBONOCO)/TABLA->F_HORASM)/2) + WUNDBNGM  )/TABLA->F_HORASM)*1.66 TO WSTGMBS
      STORE  WSTGN* ( (WBASICO+WBONOCO+TABLA->F_AYUCIU+ (((WBASICO+WBONOCO)/TABLA->F_HORASN)/2) + WUNDBNGN  )/TABLA->F_HORASN)*1.66 TO WSTGNBS
      STORE WSTGM+WSTGN     TO WSTGHR
      STORE WSTGMBS+WSTGNBS TO WSTGBS

      **** TIEMPO DE VIAJE FIJO PARA SALARIO NORMAL
      IF PERSONAL->TV1>0
         STORE PERSONAL->TV1  TO WTVF
         STORE ((WBASICO+WBONOCO)/TABLA->F_HORASD)*TABLA->F_TVA*(PERSONAL->TV1*WDIASTRAB) TO WTVF
      ELSE
         IF PERSONAL->TV2>0
            STORE PERSONAL->TV2  TO WTVF
            STORE ((WBASICO+WBONOCO)/TABLA->F_HORASM)*TABLA->F_TVA*(PERSONAL->TV2*WDIASTRAB) TO WTVF
         ELSE
            IF PERSONAL->TV3>0
               STORE PERSONAL->TV3  TO WTVF
               STORE ((WBASICO+WBONOCO)/TABLA->F_HORASN)*TABLA->F_TVA*(PERSONAL->TV3*WDIASTRAB) TO WTVF
            ELSE
               STORE 0 TO WTVF
            ENDIF
         ENDIF
      ENDIF

      **** COMIDAS
      STORE  PROCESO->P_COMIDA TO WCOMIDA
      STORE  WCOMIDA*TABLA->F_COMIDA TO WCOMIDABS

      *** CALCULO DEL SALARIO NORMAL
      *IF (PERSONAL->TURNO = "G" .AND. (WDIASTRABM+WDIASTRABN) > 0 ) .OR. (PERSONAL->TV1+PERSONAL->TV2+PERSONAL->TV3>0)
         STORE .T. TO WFLAGSN
         STORE TABLA->F_DIAS-NOMINA->FERORD TO WWDIASREAL
         STORE ( WBSORD+WTVF+WSTGBS+(WDIASTRAB*TABLA->F_AYUCIU)+WCOMIDABS+WREPCOMBS+WBNGBS+(WBONOCO*WDIASTRAB) )/ WDIASTRAB  TO WSALNOR
      *ELSE
      *   STORE .F. TO WFLAGSN
      *   STORE WBASICO TO WSALNOR
      *ENDIF
      STORE WSALNOR/TABLA->F_HORASD TO WSALNORHRD
      STORE WSALNOR/TABLA->F_HORASM TO WSALNORHRM
      STORE WSALNOR/TABLA->F_HORASN TO WSALNORHRN

      **** SOBRE TIEMPO ****
      STORE 1.66         TO WFACSTA1
      STORE 1.66         TO WFACSTB1
      STORE TABLA->F_STA TO WFACSTA2
      STORE TABLA->F_STB TO WFACSTB2
      STORE WSALHRSD+(WBONOCO/TABLA->F_HORASD) TO WSALSTD2
      STORE WSALHRSM+(WBONOCO/TABLA->F_HORASM) TO WSALSTM2
      STORE WSALHRSN+(WBONOCO/TABLA->F_HORASN) TO WSALSTN2
      * DIURNO A
      STORE PROCESO->P_STDA            TO WSTDA
      STORE WSALNORHRD*WSTDA*WFACSTA1  TO WSTDABS1
      STORE WSALSTD2  *WSTDA*WFACSTA2  TO WSTDABS2
      IF PERSONAL->CONTRATO="PETROLERO"
         IF WSTDABS1>WSTDABS2
            STORE WSTDABS1 TO WSTDABS
         ELSE
            STORE WSTDABS2 TO WSTDABS
         ENDIF
      ELSE
         IF PERSONAL->CONTRATO="LDT"
            STORE WSTDABS2*1.3  TO WSTDABS
         ELSE
            STORE WSTDABS2      TO WSTDABS
         ENDIF
      ENDIF
      * MIXTO A
      STORE PROCESO->P_STMA           TO WSTMA
      STORE WSALNORHRM*WSTMA*WFACSTA1 TO WSTMABS1
      STORE WSALSTM2  *WSTMA*WFACSTA2 TO WSTMABS2
      IF PERSONAL->CONTRATO="PETROLERO"
         IF WSTMABS1>WSTMABS2
            STORE WSTMABS1 TO WSTMABS
         ELSE
            STORE WSTMABS2 TO WSTMABS
         ENDIF
      ELSE
         IF PERSONAL->CONTRATO="LDT"
            STORE WSTMABS2*1.3  TO WSTMABS
         ELSE
            STORE WSTMABS2      TO WSTMABS
         ENDIF
      ENDIF
      * NOCTURNO A
      STORE PROCESO->P_STNA           TO WSTNA
      STORE WSALNORHRN*WSTNA*WFACSTA1 TO WSTNABS1
      STORE WSALSTN2  *WSTNA*WFACSTA2 TO WSTNABS2
      IF PERSONAL->CONTRATO="PETROLERO"
         IF WSTNABS1>WSTNABS2
            STORE WSTNABS1 TO WSTNABS
         ELSE
            STORE WSTNABS2 TO WSTNABS
         ENDIF
      ELSE
         IF PERSONAL->CONTRATO="LDT"
            STORE WSTNABS2*1.3  TO WSTNABS
         ELSE
            STORE WSTNABS2      TO WSTNABS
         ENDIF
      ENDIF

      * DIURNO B
      STORE PROCESO->P_STDB           TO WSTDB
      STORE WSALNORHRD*WSTDB*WFACSTB1 TO WSTDBBS1
      STORE WSALSTD2  *WSTDB*WFACSTB2 TO WSTDBBS2
      IF PERSONAL->CONTRATO="PETROLERO"
         IF WSTDBBS1>WSTDBBS2
            STORE WSTDBBS1 TO WSTDBBS
         ELSE
            STORE WSTDBBS2 TO WSTDBBS
         ENDIF
      ELSE
         IF PERSONAL->CONTRATO="LDT"
            STORE WSTDBBS2*1.3  TO WSTDBBS
         ELSE
            STORE WSTDBBS2      TO WSTDBBS
         ENDIF
      ENDIF
      * MIXTO B
      STORE PROCESO->P_STMB           TO WSTMB
      STORE WSALNORHRM*WSTMB*WFACSTB1 TO WSTMBBS1
      STORE WSALSTM2  *WSTMB*WFACSTB2 TO WSTMBBS2
      IF PERSONAL->CONTRATO="PETROLERO"
         IF WSTMBBS1>WSTMBBS2
            STORE WSTMBBS1 TO WSTMBBS
         ELSE
            STORE WSTMBBS2 TO WSTMBBS
         ENDIF
      ELSE
         IF PERSONAL->CONTRATO="LDT"
            STORE WSTMBBS2*1.3  TO WSTMBBS
         ELSE
            STORE WSTMBBS2      TO WSTMBBS
         ENDIF
      ENDIF
      * NOCTURNO B
      STORE PROCESO->P_STNB           TO WSTNB
      STORE WSALNORHRN*WSTNB*WFACSTB1 TO WSTNBBS1
      STORE WSALSTN2  *WSTNB*WFACSTB2 TO WSTNBBS2
      IF PERSONAL->CONTRATO="PETROLERO"
         IF WSTNBBS1>WSTNBBS2
            STORE WSTNBBS1 TO WSTNBBS
         ELSE
            STORE WSTNBBS2 TO WSTNBBS
         ENDIF
      ELSE
         IF PERSONAL->CONTRATO="LDT"
            STORE WSTNBBS2*1.3  TO WSTNBBS
         ELSE
            STORE WSTNBBS2      TO WSTNBBS
         ENDIF
      ENDIF

      **** SUMA DE LOS TIPOS DE SOBRE TIEMPO
      STORE WSTDA+ WSTMA+ WSTNA+ WSTDB+ WSTMB+ WSTNB+ WSTGM+ WSTGN TO WTOTSTHR
      STORE WSTDABS+WSTMABS+WSTNABS+WSTDBBS+WSTMBBS+WSTNBBS+WSTGMBS+WSTGNBS TO WTOTSTBS

      **** TIEMPO DE VIAJE
      * DIURNO DE 1.5 EN HORAS
      STORE PROCESO->P_TVDA TO WTVDA
      STORE (WSALHRSD+(WBONOCO/TABLA->F_HORASD))*WTVDA*TABLA->F_TVA TO WTVDABS
      * MIXTO DE 1.5 EN HORAS
      STORE PROCESO->P_TVMA TO WTVMA
      STORE (WSALHRSM+(WBONOCO/TABLA->F_HORASM))*WTVMA*TABLA->F_TVA TO WTVMABS
      * NOCTURNO DE 1.5 EN HORAS
      STORE PROCESO->P_TVNA TO WTVNA
      STORE (WSALHRSN+(WBONOCO/TABLA->F_HORASN))*WTVNA*TABLA->F_TVA TO WTVNABS

      * DIURNO DE +1.5 EN HORAS
      STORE PROCESO->P_TVDB TO WTVDB
      STORE (WSALHRSD+(WBONOCO/TABLA->F_HORASD))*WTVDB*TABLA->F_TVB TO WTVDBBS
      * MIXTO DE +1.5 EN HORAS
      STORE PROCESO->P_TVMB TO WTVMB
      STORE (WSALHRSM+(WBONOCO/TABLA->F_HORASM))*WTVMB*TABLA->F_TVB TO WTVMBBS
      * NOCTURNO DE +1.5 EN HORAS
      STORE PROCESO->P_TVNB TO WTVNB
      STORE (WSALHRSN+(WBONOCO/TABLA->F_HORASN))*WTVNB*TABLA->F_TVB TO WTVNBBS

      **** SUMA DE LOS TRES TIPOS DE T.V.
      STORE WTVDA+WTVMA+WTVNA+WTVDB+WTVMB+WTVNB TO WTOTTVHR
      STORE WTVDABS+WTVMABS+WTVNABS+WTVDBBS+WTVMBBS+WTVNBBS TO WTOTTVBS

      ****  DESCANSO NORMAL
      IF WFLAGMAQ
         STORE PROCESO->P_DESNOR*TABLA->F_HORASD TO WDESNORHR
         STORE PROCESO->P_DESNOR*WBASICO+WBONOCO TO WDESNORBS
      ELSE
         STORE PROCESO->P_DESNOR*TABLA->F_HORASD TO WDESNORHR
         STORE PROCESO->P_DESNOR*WSALNOR         TO WDESNORBS
      ENDIF

      ****  DESCANSO CONTRACTUAL
      STORE PROCESO->P_DESCON*TABLA->F_HORASD*TABLA->F_DESCON TO WDESCONHR
      STORE WDESCONHR*((WBASICO+WBONOCO)/TABLA->F_HORASD)               TO WDESCONBS

      ****  DESCANSO LEGAL
      STORE PROCESO->P_DESLEG*TABLA->F_HORASD*TABLA->F_DESLEG TO WDESLEGHR
      STORE WDESLEGHR*((WBASICO+WBONOCO)/TABLA->F_HORASD)               TO WDESLEGBS

      **** DIAS COMPENSATORIOS
      STORE 0 TO WDC
      STORE 0 TO WDCBS
      IF PROCESO->P_DESCOM > 0 .AND. TABLA->F_DESCOM > 0
         STORE PROCESO->P_DESCOM*TABLA->F_HORASD*TABLA->F_DESCOM TO WDC
         STORE WDC*(WSALNOR/TABLA->F_HORASD)*TABLA->F_DESCOM     TO WDCBS
      ENDIF

      **** AYUDA DE CIUDAD
      STORE PROCESO->P_AYUCIU                           TO WAYUCIU
      STORE WAYUCIU*TABLA->F_HORASD                     TO WAYUCIUHR
      STORE WAYUCIUHR*(TABLA->F_AYUCIU/TABLA->F_HORASD) TO WAYUCIUBS

      **** FERIADOS
      STORE PROCESO->P_FERORD*TABLA->F_HORASD  TO WFERHORAS
      STORE PROCESO->P_FERORD*WSALNOR          TO WFERTOT

      **** BONO NOCTURNO TRABAJADO
      STORE  PROCESO->P_BNT TO WBN
      STORE  WSALNORHRN*TABLA->F_BNOCTU*WBN TO WBNBS

      **** BONO NOCTURNO DE VIAJE
      STORE  PROCESO->P_BNV TO WBNV
      STORE  ((WBASICO+WBONOCO)/TABLA->F_HORASD)*TABLA->F_BNVIAJE*WBNV TO WBNVBS

      **** BONO DOMINICAL
      STORE  PROCESO->P_BD*TABLA->F_BDOMIN*TABLA->F_HORASD TO WBDHR
      STORE  WBDHR*((WBASICO+WBONOCO)/TABLA->F_HORASD)               TO WBDBS

      **** PRIMA POR FERIADO TRABAJADO
      STORE PROCESO->P_FERPRID+PROCESO->P_FERPRIM+PROCESO->P_FERPRIN TO WFERPRI
      STORE WFERPRI*TABLA->F_BDOMIN*TABLA->F_HORASD                  TO WFERPRIHR
      STORE WFERPRIHR*((WBASICO+WBONOCO)/TABLA->F_HORASD)                      TO WFERPRIBS

      **** BONO COMPENSATORIO
      STORE WHORASORD+WHORASORM+WHORASORN TO WBCHR
      IF .NOT. WFLAGSN
         STORE WBCHR+WDESNORHR+WFERHORAS TO WBCHR
      ENDIF
      STORE (WBONOCO/TABLA->F_HORASD)*WBCHR TO WBCBS
      IF WBCBS = 0
         STORE 0 TO WBCHR
      ENDIF

      STORE PROCESO->P_DES3 TO   WDES3
      STORE PROCESO->P_DES4 TO   WDES4
      STORE PROCESO->P_OCB3 TO WOCBBS3
      STORE PROCESO->P_OCB4 TO WOCBBS4

      SELECT 2
      ****************** T O T A L    B O N I F I C A B L E *****************
      STORE WBSORDD+WBSORDM+WBSORDN+WTOTSTBS+WTOTTVBS+WDESNORBS+WDESLEGBS+;
            WDESCONBS+WDCBS+WAYUCIUBS+WCOMIDABS+WREPCOMBS+WFERTOT+WBNBS+;
            WBNVBS+WBDBS+WFERPRIBS+WBNGBS+WBCBS+WOCBBS3+WOCBBS4 TO WTOTBON

      STORE WTOTBON TO VARIABLE
      DO REDONDEO WITH VARIABLE
      STORE VARIABLE TO WTOTBON
      store round(wtotbon,2) to wtotbon

      ************************************************************************
      ******                CONCEPTOS NO BONIFICABLES                   ******
      ************************************************************************

      *** BONO VACACIONAL PRORRATEADO PARA OBREROS TEMPORALES
      *** SEGUN NEWSCA
      IF PERSONAL->TIPO = "OT" .AND. PERSONAL->BONOVACFRA = "S"
         STORE  WDIASTRAB              TO WBV
         STORE  WDIASTRAB*(10000/360)  TO WBVBS
      ELSE
         STORE 0 TO WBV
         STORE 0 TO WBVBS
      ENDIF

      **** CALCULO DE CESTA BASICA ****
      STORE 0 TO WCESTA
      IF TABLA->F_CESTA > 0 .AND. UPPER(PERSONAL->CESTA) = "S"
         IF PROCESO->P_FLAGCES1 = "S"
            IF (NOMINA->APERT2 - PERSONAL->INGRESO) + 1 > 20
               STORE TABLA->F_CESTA TO WCESTA
            ELSE
               IF NOMINA->APERT2 - PERSONAL->INGRESO >= 5
                  STORE TABLA->F_CESTA/2 TO WCESTA
               ENDIF
            ENDIF
         ELSE
            IF PROCESO->P_FLAGCES2 = "S"
               STORE DAY(NOMINA->APERT2) TO WDIASCEST
               IF WDIASCEST > 20
                  STORE TABLA->F_CESTA TO WCESTA
               ELSE
                  IF WDIASCEST >= 5
                     STORE TABLA->F_CESTA/2 TO WCESTA
                  ENDIF
               ENDIF
            ENDIF
         ENDIF
      ENDIF

      ****  PAGO POR CONCEPTO DE VIVIENDA
      STORE PROCESO->P_VIVE TO WVIVE
      STORE WVIVE*TABLA->F_VIVIEN TO WVIVEBS

      ****  PAGO POR CONCEPTO DE BONO DE TRANSPORTE
      STORE PROCESO->P_BTRAN*PERSONAL->BT TO WBONOTRAN

      ****  PAGO POR CONCEPTO DE BONO ALIMENTICIO
      STORE PROCESO->P_BALIM*PERSONAL->BA TO WBONOALIM

      ****  PAGO POR CONCEPTO DE BONO SUBSIDIO
      STORE PROCESO->P_BSUBS*PERSONAL->BS TO WBONOSUBS

      *** UTILIDADES e INDEMNIZACION
      IF PERSONAL->TIPO = "OT"
         IF P_UTILIDAD="S"
            STORE WTOTBON*(33.33/100)         TO WUTILIDAD
         ELSE
            STORE 0 TO WUTILIDAD
         ENDIF
         IF P_INDEMNI="S"
            STORE WBSORD*(33.33/100)          TO WINDEMNIZA
         ELSE
            STORE 0 TO WINDEMNIZA
         ENDIF
      ELSE
         STORE 0 TO WUTILIDAD
         STORE 0 TO WINDEMNIZA
      ENDIF

      ****  OTROS CONCEPTOS NO BONIFICABLES
      STORE PROCESO->P_DESC3 TO WDESC3
      STORE PROCESO->P_OCNB3 TO WOCNB3
      STORE PROCESO->P_DESC4 TO WDESC4
      STORE PROCESO->P_OCNB4 TO WOCNB4

      IF WBVBS > 0
         IF WOCNB4 > 0
            STORE WDESC4+"BV" TO WDESC4
            STORE WOCNB4+WBVBS TO WOCNB4
         ELSE
            STORE "BV"  TO WDESC4
            STORE WBVBS TO WOCNB4
         ENDIF
      ENDIF

      *** TOTAL NO BONIFICABLE ****

      STORE WCESTA+WVIVEBS+WBONOALIM+WBONOTRAN+WBONOSUBS+WOCNB3+WOCNB4+WUTILIDAD+WINDEMNIZA TO WTOTNBON
      
      STORE WTOTNBON TO VARIABLE
      DO REDONDEO WITH VARIABLE
      STORE VARIABLE TO WTOTNBON
      store round(wtotnbon,2) to wtotnbon

      ************************************************************************
      ************** D  E  D  U  C  C  I  O  N  E  S  ************************
      ************************************************************************
      **** ISLR
      STORE ((WTOTBON+WTOTNBON)*PERSONAL->ISLR)/100 TO WISLR
      STORE WISLR TO VARIABLE
      DO REDONDEO WITH VARIABLE
      STORE VARIABLE TO WISLR
      STORE ROUND(WISLR,2) TO WISLR

      **** SSO
      STORE ((WBASICO+WBONOCO+TABLA->F_AYUCIU)*(TABLA->F_DIAS+TABLA->F_DESNOR))*(PERSONAL->SSO/100) TO WSSO
      STORE WSSO TO VARIABLE
      DO REDONDEO WITH VARIABLE
      STORE VARIABLE TO WSSO
      STORE ROUND(WSSO,2) TO WSSO
      IF WSSO > PERSONAL->SSOMAX
         STORE PERSONAL->SSOMAX TO WSSO
      ENDIF

      **** SPF
      STORE ((WBASICO+WBONOCO+TABLA->F_AYUCIU)*(TABLA->F_DIAS+TABLA->F_DESNOR))*(PERSONAL->SPF/100) TO WSPF
      STORE WSPF TO VARIABLE
      DO REDONDEO WITH VARIABLE
      STORE VARIABLE TO WSPF
      STORE ROUND(WSPF,2) TO WSPF
      IF WSPF > PERSONAL->SPFMAX
         STORE PERSONAL->SPFMAX TO WSPF
      ENDIF

      **** AHORRO HABITACIONAL
      STORE ((WBASICO+WBONOCO)*(TABLA->F_DIAS+TABLA->F_DESNOR))*(PERSONAL->HAB/100) TO WAHRHAB
      STORE WAHRHAB TO VARIABLE
      DO REDONDEO WITH VARIABLE
      STORE VARIABLE TO WAHRHAB
      STORE ROUND(WAHRHAB,2) TO WAHRHAB

      **** CAJA DE AHORRO
      STORE ((WBASICO+WBONOCO+TABLA->F_AYUCIU)*(TABLA->F_DIAS+TABLA->F_DESNOR))*(PERSONAL->CAJA/100) TO WCAJA
      STORE WCAJA TO VARIABLE
      DO REDONDEO WITH VARIABLE
      STORE VARIABLE TO WCAJA
      STORE ROUND(WCAJA,2) TO WCAJA

      **** PRESTAMO PERSONAL
      STORE PROCESO->P_PRESPER TO WPRESPER

      **** SINDICATO
      IF PERSONAL->CONTRATO = "CONSTRUCCION"
         STORE (PERSONAL->SUELDO*(TABLA->F_DIAS+TABLA->F_DESNOR))*(PERSONAL->SINDI/100)        TO WSINDIC
      ELSE
         STORE PERSONAL->SINDI        TO WSINDIC
      ENDIF
      if personal->sindicato = space(12)
         store 0 to wsindic
      endif

      **** OTRAS DEDUCCIONES
      STORE PROCESO->P_DESCR3        TO WDESCR3
      STORE PROCESO->P_DESCR4        TO WDESCR4
      STORE PROCESO->P_OD3           TO WOD3
      STORE PROCESO->P_OD4           TO WOD4
      ******  TOTAL  DEDUCCIONES
      STORE WISLR+WSSO+WSPF+WPRESPER+WSINDIC+WOD3+WOD4+WAHRHAB+WCAJA TO WTOTDEC
      store round(wtotdec,2) to wtotdec

      ***  P  A  G  O     N  E  T  O  ****
      STORE WTOTBON+WTOTNBON-WTOTDEC TO WNETOPAGAR
   RETURN

